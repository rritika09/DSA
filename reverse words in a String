# Online Python compiler (interpreter) to run Python online.
# Write Python 3 code in this online editor and run it.
print("Hello world")

#Method 1 : List 
str = "Life is beautiful"
stck = str.split()   # split converts string to list
print(type(stck))    # print list
print(stck)
stck_o = []
print(len(stck))
for i in range(len(stck)-1, -1, -1): #range(start,stop,step)
    stck_o.append(stck[i]) #stop is not included, start is included
print(stck_o)

#Method 2 : List as Stack
st = "world is round"
st_lst=st.split()
print("st_lst",st_lst)
newst = st.split()[::-1]
out = []
print(newst)
# for i in range(len(newst)):
#     out.append(newst[i])
# print(out)
print(" ".join(newst))
print("Second method")
for i in range(len(st_lst)):
    out.append(st_lst.pop())
print("through stack",out)
print(" ".join(out))
